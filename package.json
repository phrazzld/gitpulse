{
  "name": "gitpulse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXTAUTH_URL=http://localhost:3000 NEXT_PUBLIC_GITHUB_APP_NAME=pulse-summarizer next dev",
    "dev:log": "npm run logs:rotate && NEXTAUTH_URL=http://localhost:3000 NEXT_PUBLIC_GITHUB_APP_NAME=pulse-summarizer LOG_LEVEL=debug next dev 2>&1 | tee -a ./logs/app.log",
    "dev:debug": "NEXTAUTH_URL=http://localhost:3000 NEXT_PUBLIC_GITHUB_APP_NAME=pulse-summarizer LOG_LEVEL=debug next dev",
    "logs:rotate": "./scripts/rotate-logs.sh",
    "logs:clean": "rm -rf ./logs/*.log",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "chromatic": "npx chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@octokit/auth-app": "^6.0.0",
    "next": "15.2.4",
    "next-auth": "^4.24.11",
    "octokit": "^4.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/nextjs": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/test-runner": "^0.22.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-window": "^1.8.8",
    "axe-playwright": "^2.1.0",
    "chromatic": "^11.28.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.2",
    "eslint-plugin-storybook": "^0.12.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "storybook": "^8.6.12",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  }
}
