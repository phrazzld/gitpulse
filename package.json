{
  "name": "gitpulse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXTAUTH_URL=http://localhost:3000 NEXT_PUBLIC_GITHUB_APP_NAME=pulse-summarizer next dev",
    "dev:log": "npm run logs:rotate && NEXTAUTH_URL=http://localhost:3000 NEXT_PUBLIC_GITHUB_APP_NAME=pulse-summarizer LOG_LEVEL=debug next dev 2>&1 | tee -a ./logs/app.log",
    "dev:debug": "NEXTAUTH_URL=http://localhost:3000 NEXT_PUBLIC_GITHUB_APP_NAME=pulse-summarizer LOG_LEVEL=debug next dev",
    "logs:rotate": "./scripts/rotate-logs.sh",
    "logs:clean": "rm -rf ./logs/*.log",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test:react": "jest --config jest.config.react.js",
    "test:react:watch": "jest --config jest.config.react.js --watch",
    "test:react:coverage": "jest --config jest.config.react.js --coverage",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@octokit/auth-app": "^6.0.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "next": "15.2.4",
    "next-auth": "^4.24.11",
    "octokit": "^4.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-window": "^1.8.11",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/nextjs": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-window": "^1.8.8",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.2",
    "eslint-plugin-storybook": "^0.12.0",
    "husky": "^9.1.7",
    "storybook": "^8.6.12",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  }
}
