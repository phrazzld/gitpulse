# GitHub OAuth credentials (required for personal auth)
GITHUB_OAUTH_CLIENT_ID=your_github_client_id
GITHUB_OAUTH_CLIENT_SECRET=your_github_client_secret

# GitHub App credentials (optional - only needed for GitHub App authentication)
GITHUB_APP_ID=your_github_app_id
GITHUB_APP_PRIVATE_KEY_PKCS8=your_github_app_private_key
NEXT_PUBLIC_GITHUB_APP_NAME=your_github_app_name

# NextAuth.js settings
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# Gemini API settings
GEMINI_API_KEY=your_gemini_api_key